{
    "name": "jackbrycesmith/featica",
    "homepage": "https://github.com/jackbrycesmith/featica",
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Bryce-Smith"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "inertiajs/inertia-laravel": "^0.3.5"
    },
    "require-dev": {
        "claudiodekker/inertia-laravel-testing": "^2.2",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^0.3.19",
        "pestphp/pest-plugin-expectations": "^0.3.3",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Featica\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Featica\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "pest --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Featica\\FeaticaServiceProvider"
            ],
            "aliases": {
                "Featica": "Featica\\FeaticaFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
